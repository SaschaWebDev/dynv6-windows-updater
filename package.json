	"dependencies": {
		"@types/express": "^4.17.14",
		"@types/helmet": "^4.0.0",
		"@types/morgan": "^1.9.3",
		"@types/node": "^18.7.23",
		"concurrently": "^7.4.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.1",
		"helmet": "^6.0.0",
		"morgan": "^1.10.0",
		"rimraf": "^3.0.2",
